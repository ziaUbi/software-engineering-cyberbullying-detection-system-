{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "maxNumMissingSamples": {
      "type": "integer",
      "minimum": 0
    },
    "current_phase": {
      "type": "string",
      "enum": ["development", "production", "evaluation"]
    },
    "service": {
      "type": "boolean"
    },
    "production_sessions": {
      "type": "integer",
      "minimum": 0
    },
    "development_sessions": {
      "type": "integer",
      "minimum": 0
    },
    "evaluation_sessions": {
      "type": "integer",
      "minimum": 0
    },
    "ip_preparation": {
      "type": "string",
      "format": "ipv4"
    },
    "ip_evaluation": {
      "type": "string",
      "format": "ipv4"
    },
    "port_preparation": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535
    },
    "port_evaluation": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535
    },
    "ip_ingestion": {
      "type": "string",
      "format": "ipv4"
    },
    "port_ingestion": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535
    }
  },
  "required": [
    "maxNumMissingSamples",
    "current_phase",
    "service",
    "production_sessions",
    "development_sessions",
    "evaluation_sessions",
    "ip_preparation",
    "ip_evaluation",
    "port_preparation",
    "port_evaluation",
    "ip_ingestion",
    "port_ingestion"
  ]
}